<h1>Create a User! </h1>
<%= form_for(@user) do |f| %>
<div>
<%= f.label :name %>
<%= f.text_field :name %>
</div>
<div>
<%= f.label :age %>
<%= f.text_field :age %>
</div>
<div>
<%= f.label :email %>
<%= f.text_field :email %>
</div>
<div>
<%= f.label :zipcode %>
<%= f.number_field :zipcode %>
</div>
<div>
<%= f.label :password %>
<%= f.password_field :password %>
</div>
<%= f.submit %>
<% end %>
